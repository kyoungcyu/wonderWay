<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
namespace : xml 파일이 여러개일 수 있음
이를 구별하기 위한 식별 용도로 사용됨
 -->
<mapper namespace="kr.or.ddit.util.mapper.UtilMapper">
	<select id="commomCodeSearch" parameterType="String" resultType="String">
		SELECT CM_DE_NM
		FROM CM_DE
		WHERE CM_DE_CODE = #{cmDeCode}
	</select>
</mapper>
