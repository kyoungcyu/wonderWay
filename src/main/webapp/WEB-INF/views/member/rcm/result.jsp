<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags"%>
<head>
<!-- Meta Tags -->
<meta charset="utf-8">
<meta name="viewport" content="width=devce-width, initial-scale=1, shrink-to-fit=no">
<meta name="author" content="Webestica.com">
<meta name="description" content="Booking - Multipurpose Online Booking Theme">


</head>

<body>

	<!-- **************** MAIN CONTENT START **************** -->
	<main>

		<!-- Page main content START -->
		<div class="page-content-wrapper p-xxl-4">

			<!-- Filters START -->
			<div class="col-lg-5" style="margin-left: 590px;">
				<h1 class="h3 mb-3 mb-sm-0">여행지 추천 결과</h1>
			</div>
			
			<!-- 검색 -->
			<div class="d-flex align-items-center col-md-6 col-lg-2">
			</div>
			
			<!-- 기간 설정 -->
			<div class="d-flex align-items-center col-md-6 col-lg-3">
			</div>
			
			<!-- 정렬 -->
			<div class="col-md-6 col-lg-2">
			</div>
			<!-- Filters END -->

			<!-- Adv list START -->
			<div class="p-3" style="position: relative;">
			</div>
			
			<div class="row g-4">
				<div class="col-lg-3 div3" style="float: left; width: 33%;">
					<div class="card shadow">
						<div class="card-header border-bottom" style="text-align: center;">
							<h5 class="card-header-title">${param.predict1}</h5>
						</div>
						<div class="card-body" style="text-align: center;">
							<ul class='list-group list-group-borderless'>
								<li class='list-group-item mb-3'> 
									<label class="form-label">지역 </label> <span class='h6 fw-normal ms-1 mb-0'>${predict1.tourLoc}</span>
								</li>
								<li class='list-group-item mb-3'> 
									<label class="form-label">분류</label> <span class='h6 fw-normal ms-1 mb-0'>${predict1.tourCode}</span>
								</li>
								<li class='list-group-item mb-3'> 
									<label class="form-label">소개</label> <span class='h6 fw-normal ms-1 mb-0'>${predict1.tourDc}</span>
								</li>
							</ul>
						</div>
					</div>
				</div>
				<div class="col-lg-3 div3" style="float: left; width: 33%;">
					<div class="card shadow">
						<div class="card-header border-bottom" style="text-align: center;">
							<h5 class="card-header-title">${param.predict2}</h5>
						</div>
						<div class="card-body" style="text-align: center;">
							<ul class='list-group list-group-borderless'>
								<li class='list-group-item mb-3'> 
									<label class="form-label">지역 </label> <span class='h6 fw-normal ms-1 mb-0'>${predict2.tourLoc}</span>
								</li>
								<li class='list-group-item mb-3'> 
									<label class="form-label">분류</label> <span class='h6 fw-normal ms-1 mb-0'>${predict2.tourCode}</span>
								</li>
								<li class='list-group-item mb-3'> 
									<label class="form-label">소개</label> <span class='h6 fw-normal ms-1 mb-0'>${predict2.tourDc}</span>
								</li>
							</ul>
						</div>
					</div>
				</div>
				<div class="col-lg-3 div3" style="float: left; width: 33%;">
					<div class="card shadow">
						<div class="card-header border-bottom" style="text-align: center;">
							<h5 class="card-header-title">${param.predict3}</h5>
						</div>
						<div class="card-body" style="text-align: center;">
							<ul class='list-group list-group-borderless'>
								<li class='list-group-item mb-3'> 
									<label class="form-label">지역 </label> <span class='h6 fw-normal ms-1 mb-0'>${predict3.tourLoc}</span>
								</li>
								<li class='list-group-item mb-3'> 
									<label class="form-label">분류</label> <span class='h6 fw-normal ms-1 mb-0'>${predict3.tourCode}</span>
								</li>
								<li class='list-group-item mb-3'> 
									<label class="form-label">소개</label> <span class='h6 fw-normal ms-1 mb-0'>${predict3.tourDc}</span>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
			<!-- Guest list END -->
		</div>
		<!-- Page main content END -->
	</main>
	<!-- **************** MAIN CONTENT END **************** -->
</body>
</html>